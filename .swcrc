{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": false,
      "dynamicImport": true,
      "privateMethod": true,
      "functionBind": false,
      "exportDefaultFrom": true,
      "exportNamespaceFrom": true,
      "decorators": false,
      "decoratorsBeforeExport": false,
      "topLevelAwait": true,
      "importMeta": true
    },
    "target": "es2022",
    "loose": true,
    "externalHelpers": false,
    "paths": {
      "@manager": [
        "manager/index"
      ],
      "@common/*": [
        "common/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@bot": [
        "bot"
      ]
    }
  },
  "module": {
    "lazy": true,
    "strict": true,
    "noInterop": false,
    "strictMode": true,
    "type": "commonjs"
  },
  "minify": true
}